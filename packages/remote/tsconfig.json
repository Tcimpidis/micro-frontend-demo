{
    "compilerOptions": {
      "jsx": "react-jsx",
      "target": "es6",
      "lib":["dom", "dom.iterable", "esnext"],
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "module": "esnext",
      "moduleResolution": "node"
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
  }